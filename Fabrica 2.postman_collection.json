							"  const region = pm.environment.get(\"AWS_REGION\") || \"sa-east-1\";",
							"  const id = crypto.randomUUID();",
							"  return `https://${bucket}.s3.${region}.amazonaws.com/points/${id}.jpg`;",
							"}",
							"",
							"const count = Math.floor(Math.random() * 4) + 3; // 3 a 6 pontos",
							"const points = [];",
							"",
							"for (let i = 0; i < count; i++) {",
							"  points.push({",
							"    name: sampleNames[Math.floor(Math.random() * sampleNames.length)],",
							"    description: sampleDescriptions[Math.floor(Math.random() * sampleDescriptions.length)],",
							"    type: types[Math.floor(Math.random() * types.length)],",
							"    latitude: randomLat().toString(),",
							"    longitude: randomLng().toString(),",
							"    images: [",
							"      {",
							"        \"fileName\": randomImageUrl(),",
							"        \"imageUrl\": randomImageUrl()",
							"      }",
							"    ]",
							"  });",
							"}",
							"",
							"let images = {",
							"    \"fileName\": randomImageUrl(),",
							"    \"imageUrl\": randomImageUrl()",
							"}",
							"",
							"pm.environment.set(\"images\", JSON.stringify(images));",
							"pm.environment.set(\"mockPoints\", JSON.stringify(points));",
							"console.log(\"Mock points gerados:\", images);"
